						<div class="box-container-login-div-main">
							<p class="login-word">Forget The Password</p>
							<a href="~/Login/Register" class="register-word">No account? register</a>
						</div>
						<div class="box-container-login">
							<div class="box-container-login-div">
								<p class="first-word">Username:</p>
								<input type="text" name="username" id="username" required />
								<a href="~/Login/Register" class="second-word">register</a>
							</div>
							<div class="box-container-login-div">
								<p class="first-word">Email:</p>
								<input type="email" name="gmail" id="gmail" required />
							</div>
							<div class="box-container-login-div">
								<p class="first-word">New Password:</p>
								<input type="password" name="password" id="password" required />
							</div>
							<div class="box-container-login-div">
								<p class="first-word">Confirm Password:</p>
								<input type="password" name="cpassword" id="cpassword" required />
							</div>
							<div class="button-login">
								<button type="submit">Confirm</button>
							</div>
						</div>
					</div>
				</form>
			</section>
